def find_max_min(arr):
    if not arr:  # Check if the array is empty
        return None, None
    
    min_val = arr[0]  # First element is the minimum
    max_val = arr[-1]  # Last element is the maximum
    
    return min_val, max_val

# Test cases
print("Test Case 1:")
N = 8
a = [2, 4, 6, 8, 10, 12, 14, 18]
min_val, max_val = find_max_min(a)
print(f"Input: N = {N}, a = {a}")
print(f"Output: Min = {min_val}, Max = {max_val}")

print("\nTest Case 2:")
N = 9
a = [11, 13, 15, 17, 19, 21, 23, 35, 37]
min_val, max_val = find_max_min(a)
print(f"Input: N = {N}, a = {a}")
print(f"Output: Min = {min_val}, Max = {max_val}")

print("\nTest Case 3:")
N = 10
a = [22, 34, 35, 36, 43, 67, 12, 13, 15, 17]
min_val, max_val = find_max_min(a)
print(f"Input: N = {N}, a = {a}")
print(f"Output: Min = {min_val}, Max = {max_val}")
